                function EnforceLength(txtbox, max, e) {
                    var KeyID
                    if (navigator.appName.indexOf("Microsoft") > -1) {
                        KeyID = window.event.keyCode
                    } else {
                        KeyID = e.charCode
                    }                
                                                       
                    if (KeyID != 0) {
                        if (txtbox.value.length >= max) {
                            return false
                        }                                
